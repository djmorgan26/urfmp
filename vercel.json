{
  "version": 2,
  "name": "urfmp-web",
  "buildCommand": "cd web && npm ci && npm run build:vercel",
  "outputDirectory": "web/dist",
  "installCommand": "echo 'Dependencies installed in buildCommand'",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_COMPANY_NAME": "URFMP",
    "VITE_PRODUCT_NAME": "URFMP",
    "VITE_PRODUCT_FULL_NAME": "Universal Robot Fleet Management Platform",
    "VITE_TAGLINE": "The Stripe of Robotics",
    "VITE_DESCRIPTION": "Monitor any robot in 7 lines of code",
    "VITE_DEMO_MODE": "true",
    "VITE_URFMP_API_KEY": "demo-frontend-key",
    "VITE_URFMP_API_URL": "",
    "VITE_URFMP_WS_URL": ""
  }
}