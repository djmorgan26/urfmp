{
  "name": "@urfmp/types",
  "version": "1.0.0",
  "description": "Shared TypeScript types for URFMP",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc --skipLibCheck --outDir dist --declaration --sourceMap --declarationMap --types node src/index.ts",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build"
  },
  "keywords": [
    "typescript",
    "types",
    "robotics",
    "urfmp"
  ],
  "author": "URFMP Team",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "files": [
    "dist",
    "src"
  ]
}
